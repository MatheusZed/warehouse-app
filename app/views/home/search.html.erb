<div class="content">
  <h1>Search results</h1>
</div>
<div class="table is-striped" id="src">
  <table>
    <tbody>
      <tr>
      <% @warehouses.each do |wh| %>
        <td><%= link_to(wh.name, warehouse_path(wh.id)) %></td>
        <td><%= wh.code %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
